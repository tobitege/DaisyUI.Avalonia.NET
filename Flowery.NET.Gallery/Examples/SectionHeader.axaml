<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Flowery.NET.Gallery.Examples.SectionHeader">
    <Border BorderBrush="{DynamicResource DaisyPrimaryBrush}" BorderThickness="4,0,0,0" Padding="12,4,0,4">
        <TextBlock x:Name="TitleText" FontWeight="Bold" FontSize="20" Foreground="{DynamicResource DaisyBaseContentBrush}"/>
    </Border>
</UserControl>
